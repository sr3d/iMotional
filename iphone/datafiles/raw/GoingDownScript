grep -C 2 USERACCELERATION GoingDown > GoingDown_USERACCELERATION
grep -C 2 USERACCELERATION GoingDown1 > GoingDown1_USERACCELERATION
grep -C 2 USERACCELERATION GoingDown2 > GoingDown2_USERACCELERATION

grep -C 2 ACCELEROMETER GoingDown > GoingDown_ACCELEROMETER
grep -C 2 ACCELEROMETER GoingDown1 > GoingDown1_ACCELEROMETER
grep -C 2 ACCELEROMETER GoingDown2 > GoingDown2_ACCELEROMETER

grep -C 2 GRAVITY GoingDown > GoingDown_GRAVITY_ACCELEROMETER
grep -C 2 GRAVITY GoingDown1 > GoingDown1_GRAVITY_ACCELEROMETER
grep -C 2 GRAVITY GoingDown2 > GoingDown2_GRAVITY_ACCELEROMETER

grep -C 2 ROTATIONRATE GoingDown > GoingDown_ROTATIONRATE
grep -C 2 ROTATIONRATE GoingDown1 > GoingDown1_ROTATIONRATE
grep -C 2 ROTATIONRATE GoingDown2 > GoingDown2_ROTATIONRATE

grep -C 2 LOWPASS GoingDown > GoingDown_LOWPASS
grep -C 2 LOWPASS GoingDown1 > GoingDown1_LOWPASS
grep -C 2 LOWPASS GoingDown2 > GoingDown2_LOWPASS
