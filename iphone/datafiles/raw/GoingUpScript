grep -C 2 USERACCELERATION GoingUp > GoingUp_USERACCELERATION
grep -C 2 USERACCELERATION GoingUp1 > GoingUp1_USERACCELERATION
grep -C 2 USERACCELERATION GoingUp2 > GoingUp2_USERACCELERATION

grep -C 2 ACCELEROMETER GoingUp > GoingUp_ACCELEROMETER
grep -C 2 ACCELEROMETER GoingUp1 > GoingUp1_ACCELEROMETER
grep -C 2 ACCELEROMETER GoingUp2 > GoingUp2_ACCELEROMETER

grep -C 2 GRAVITY GoingUp > GoingUp_GRAVITY_ACCELEROMETER
grep -C 2 GRAVITY GoingUp1 > GoingUp1_GRAVITY_ACCELEROMETER
grep -C 2 GRAVITY GoingUp2 > GoingUp2_GRAVITY_ACCELEROMETER

grep -C 2 ROTATIONRATE GoingUp > GoingUp_ROTATIONRATE
grep -C 2 ROTATIONRATE GoingUp1 > GoingUp1_ROTATIONRATE
grep -C 2 ROTATIONRATE GoingUp2 > GoingUp2_ROTATIONRATE

grep -C 2 LOWPASS GoingUp > GoingUp_LOWPASS
grep -C 2 LOWPASS GoingUp1 > GoingUp1_LOWPASS
grep -C 2 LOWPASS GoingUp2 > GoingUp2_LOWPASS
